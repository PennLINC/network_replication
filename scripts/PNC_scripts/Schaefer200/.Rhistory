connect.matrix$data[which(mask == 0), ] <- NA
connect.matrix$data[,which(mask == 0)] <- NA
#compute average connectivity
GBC <- as.array(rowMeans(connect.matrix$data, na.rm = TRUE))
return(GBC)
}
